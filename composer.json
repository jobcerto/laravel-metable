{
    "name": "jobcerto/laravel-metable",
    "description": "A package to give the possibility of add meta data to Eloquent Models",
    "keywords": [
        "jobcerto",
        "laravel-metable"
    ],
    "homepage": "https://github.com/jobcerto/laravel-metable",
    "license": "MIT",
    "authors": [
        {
            "name": "Eduardo Geschonke",
            "email": "edu@Jobcerto.com",
            "homepage": "https://jobcerto.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "5.8.*"

    },
    "require-dev": {
        "orchestra/testbench": "3.8.*",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Jobcerto\\Metable\\": "src"
        }
    },
     "autoload-dev": {
        "psr-4": {
            "Jobcerto\\Metable\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jobcerto\\Metable\\MetableServiceProvider"
            ]
        }
    }
}
